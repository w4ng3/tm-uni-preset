{
  "easycom": {
    "autoscan": true,
    "custom": {
      "^tm-(.*)": "@/tmui/components/tm-$1/tm-$1.vue"
    }
  },
  "tabBar": {
    "color": "#7A7E83",
    "selectedColor": "#3cc51f",
    "borderStyle": "black",
    "backgroundColor": "#ffffff",
    "list": [
      {
        "pagePath": "pages/about",
        "iconPath": "static/logo.png",
        "selectedIconPath": "static/logo.png",
        "text": "about"
      },
      {
        "pagePath": "pages/pullView",
        "iconPath": "static/logo.png",
        "selectedIconPath": "static/logo.png",
        "text": "pullView"
      },
      {
        "pagePath": "pages/personal/my",
        "iconPath": "static/logo.png",
        "selectedIconPath": "static/logo.png",
        "text": "personal"
      }
    ]
  },
  "pages": [
    {
      "path": "pages/about",
      "style": {
        "navigationBarTitleText": "about",
        "enablePullDownRefresh": true
      }
    },
    {
      "path": "pages/pullView",
      "style": {
        "navigationBarTitleText": "pullView",
        "enablePullDownRefresh": true
      }
    },
    {
      "path": "pages/index/index",
      "style": {
        "navigationBarTitleText": "",
        "navigationStyle": "custom",
        "titlePenetrate": "YES",
        "transparentTitle": "always"
      }
    },
    {
      "path": "pages/personal/my",
      "style": {
        "navigationBarTitleText": "my"
      }
    },
    {
      "path": "pages/personal/custom-tabbar",
      "style": {
        "navigationBarTitleText": "custom-tabbar"
      }
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#FFFFFF",
    "backgroundColor": "#FFFFFF"
  },
  "condition": {
    "current": 0,
    "list": [
      {
        "name": "",
        "path": "",
        "query": ""
      }
    ]
  }
}
